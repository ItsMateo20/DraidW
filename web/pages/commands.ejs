    <body>
        <div class="container">
            <%- include('../partials/header.ejs') %>
                <div class="headerText">
                    <h1 style="font-size: 50px;">Commands</h1>
                    <h4 style="font-size: 20px;">All of the commands that the bot offers</h4>
                </div>
                <div class="divider"></div>
                <div class="d-grid gap-2 d-md-flex text-center text-white mt-3 categories">
                    <button class="btn btn-outline-secondary" onclick="active(this)" id="CategoryBtnAll"
                        type="button">All</button>
                    <button class="btn btn-outline-secondary" onclick="active(this)" id="CategoryBtn0"
                        type="button">Currency</button>
                    <button class="btn btn-outline-secondary" onclick="active(this)" id="CategoryBtn1"
                        type="button">Music</button>
                    <button class="btn btn-outline-secondary" onclick="active(this)" id="CategoryBtn2"
                        type="button">Moderation</button>
                    <button class="btn btn-outline-secondary" onclick="active(this)" id="CategoryBtn3"
                        type="button">Fun</button>
                    <% if (loggedIn && developer) { %>
                        <button class="btn btn-outline-secondary" onclick="active(this)" id="CategoryBtnDev"
                            type="button">Developer</button>
                        <% } %>
                            <button class="btn btn-outline-secondary" onclick="active(this)" id="CategoryBtn4"
                                type="button">Games</button>
                            <button class="btn btn-outline-secondary" onclick="active(this)" id="CategoryBtn5"
                                type="button">Settings</button>
                            <button class="btn btn-outline-secondary" onclick="active(this)" id="CategoryBtn6"
                                type="button">Slash commands</button>
                            <button class="btn btn-outline-secondary" onclick="active(this)" id="CategoryBtn7"
                                type="button">Others</button>
                </div>
                <div class="divider mt-3"></div>
                <div class="mt-4"></div>
                <div class="commands-section" id="Category0">
                    <!-- Currency -->
                    <div class="accordion accordion-flush mt-2" id="balance">
                        <div class="accordion-item">
                            <h2 class="accordion-header" id="flush-headingOne">
                                <button class="accordion-button collapsed" data-bs-toggle="collapse"
                                    data-bs-target="#fbalance"> ,balance </button>
                            </h2>
                            <div id="fbalance" class="accordion-collapse collapse" data-bs-parent="#balance">
                                <div class="accordion-body rounded">
                                    <p>
                                        <b>Description:</b> <span class="text-warning">Check your balance or
                                            somebodies.</span>
                                    </p>
                                    <p>
                                        <b>Cooldown:</b> <span class="text-success">4s</span>
                                        <b>Voted cooldown:</b> <span class="text-success">1s</span>
                                    </p>
                                    <p>
                                        <b>Usage:</b> <span class="text-danger">,balance [@user/ID]</span>
                                    </p>
                                    <p>
                                        <b>Example:</b> <span class="text-danger">,balance @ItsMateo20#1001</span>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="accordion accordion-flush mt-2" id="beg">
                        <div class="accordion-item">
                            <h2 class="accordion-header" id="flush-headingOne">
                                <button class="accordion-button collapsed" data-bs-toggle="collapse"
                                    data-bs-target="#fbeg">
                                    ,beg </button>
                            </h2>
                            <div id="fbeg" class="accordion-collapse collapse" data-bs-parent="#beg">
                                <div class="accordion-body rounded">
                                    <p>
                                        <b>Description:</b> <span class="text-warning">Beg people for money.</span>
                                    </p>
                                    <p>
                                        <b>Cooldown:</b> <span class="text-success">4s</span>
                                        <b>Voted cooldown:</b> <span class="text-success">1s</span>
                                    </p>
                                    <p>
                                        <b>Usage:</b> <span class="text-danger">,beg</span>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="accordion accordion-flush mt-2" id="buy">
                        <div class="accordion-item">
                            <h2 class="accordion-header" id="flush-headingOne">
                                <button class="accordion-button collapsed" data-bs-toggle="collapse"
                                    data-bs-target="#fbuy">
                                    ,buy </button>
                            </h2>
                            <div id="fbuy" class="accordion-collapse collapse" data-bs-parent="#buy">
                                <div class="accordion-body rounded">
                                    <p>
                                        <b>Description:</b> <span class="text-warning">Buy something from the shop by
                                            ID.</span>
                                    </p>
                                    <p>
                                        <b>Cooldown:</b> <span class="text-success">4s</span>
                                        <b>Voted cooldown:</b> <span class="text-success">1s</span>
                                    </p>
                                    <p>
                                        <b>Usage:</b> <span class="text-danger">,buy {itemID}</span>
                                    </p>
                                    <p>
                                        <b>Example:</b> <span class="text-danger">,buy 2</span>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="accordion accordion-flush mt-2" id="give">
                        <div class="accordion-item">
                            <h2 class="accordion-header" id="flush-headingOne">
                                <button class="accordion-button collapsed" data-bs-toggle="collapse"
                                    data-bs-target="#fgive"> ,give </button>
                            </h2>
                            <div id="fgive" class="accordion-collapse collapse" data-bs-parent="#give">
                                <div class="accordion-body rounded">
                                    <p>
                                        <b>Description:</b> <span class="text-warning">Give items to a member.</span>
                                    </p>
                                    <p>
                                        <b>Cooldown:</b> <span class="text-success">4s</span>
                                        <b>Voted cooldown:</b> <span class="text-success">1s</span>
                                    </p>
                                    <p>
                                        <b>Usage:</b> <span class="text-danger">,give {@user/ID} {itemID}</span>
                                    </p>
                                    <p>
                                        <b>Example:</b> <span class="text-danger">,give @Waret#1567 2</span>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="accordion accordion-flush mt-2" id="inventory">
                        <div class="accordion-item">
                            <h2 class="accordion-header" id="flush-headingOne">
                                <button class="accordion-button collapsed" data-bs-toggle="collapse"
